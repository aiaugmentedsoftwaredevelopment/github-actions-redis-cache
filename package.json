{
  "name": "redis-cache-action",
  "version": "1.0.0",
  "description": "GitHub Action for caching dependencies using Redis/Valkey",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && ncc build src/index.ts -o dist && ncc build src/post.ts -o dist-temp && mv dist-temp/index.js dist/post.js && rm -rf dist-temp",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "npm run build",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiaugmentedsoftwaredevelopment/github-actions-redis-cache.git"
  },
  "keywords": [
    "actions",
    "cache",
    "redis",
    "valkey",
    "dependencies",
    "performance"
  ],
  "author": "AI Augmented Software Development",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.3",
    "archiver": "^7.0.1",
    "ioredis": "^5.3.2",
    "tar-stream": "^3.1.7",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/tar-stream": "^3.1.4",
    "@types/unzipper": "^0.10.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
